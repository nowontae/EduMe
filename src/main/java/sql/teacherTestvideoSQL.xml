<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="teacherTestvideo">
  <select id="teacherTestvideoListN" resultType="com.edume.teacherTestvideo.model.TeacherTestvideoDTO">
  select * from testvideo where midx=#{midx} and tvcomment='N'
  </select>
  <select id="teacherTestvideoListAll" resultType="com.edume.teacherTestvideo.model.TeacherTestvideoDTO">
  select * from testvideo where midx=#{midx} and tvcomment!='N'
  </select>
  <insert id="teacherTestvideoinsert" parameterType="com.edume.teacherTestvideo.model.TeacherTestvideoDTO">
   insert into testvideo values(testvideo_idx.nextval,#{midx},#{tvsubject},#{tvvideopath},sysdate,'N')
  </insert>
  </mapper>